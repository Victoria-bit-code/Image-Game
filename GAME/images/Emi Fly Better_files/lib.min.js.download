es.--\x3e\n\x3c!-- Type 'd' to see decisioning helpers --\x3e\n",js:"/* eslint-disable no-unused-vars */\n(function(window) {\n  var _CNAME_ = 'lc-em278';\n  var _SNAME_ = 'variant-0';\n  var FLIGHT_LISTS = [\n    { el: '.outbound-list', name: 'Outbound', key: 'leg-OB' },\n    { el: '.inbound-list', name: 'Inbound', key: 'leg-IB' },\n  ];\n  var selectedClasses = {\n    'outbound': (getFlightClasses('outbound')[0] && getFlightClasses('outbound')[0].toLowerCase()) || 'economy',\n    'inbound': (getFlightClasses('inbound')[0] && getFlightClasses('inbound')[0].toLowerCase())  || 'economy',\n  };\n  var safeLog = debug(_CNAME_)(_SNAME_);\n  var $ = window.jQuery;\n\n  safeLog.info();\n\n  onDomReady(function() {\n    try {\n      addQMIntegration();\n\n      runVariant();\n\n      window.addEventListener(_CNAME_ + '-gen', runVariant);\n      window.addEventListener(_CNAME_ + '-clear', clearVariant);\n    } catch (err) {\n      safeLog.error(err);\n    }\n  });\n\n  function clearVariant() {\n    document.body.removeAttribute('data-' + _CNAME_);\n    document.body.classList.remove(_CNAME_);\n\n    // em224\n    selectedClasses = {\n      outbound: (getFlightClasses('outbound')[0] && getFlightClasses('outbound')[0].toLowerCase()) || 'economy',\n      inbound: (getFlightClasses('inbound')[0] && getFlightClasses('inbound')[0].toLowerCase())  || 'economy',\n    };\n  }\n\n  function runVariant() {\n    safeLog.ok();\n\n    document.body.classList.add(_CNAME_);\n    document.body.setAttribute('data-' + _CNAME_, _SNAME_);\n\n\n    // offers - d35\n    $('.ts-fbr-flight-list-row').each(handleFlights);\n\n    // em224\n    selectedClasses = {\n      outbound: (getFlightClasses('outbound')[0] && getFlightClasses('outbound')[0].toLowerCase()) || 'economy',\n      inbound: (getFlightClasses('inbound')[0] && getFlightClasses('inbound')[0].toLowerCase())  || 'economy',\n    };\n    renderElements();\n    filterSearchResults();\n    addListeners();\n  }\n\n  // ................................................................... offers - d35 ...................................................................\n\n  function handleFlights() {\n    var $flight = $(this);\n\n    var isGlobalMilesUpgrade = $flight.find('.js-miles-upgrade-banner').length;\n    var $globalMilesTooltips = $flight.find('.js-upgrade-cugo-container.promo-miles-upg-offer');\n    var $cugoMainTooltips = $flight.find('.js-upgrade-cugo-container');\n    var $cpmMainTooltips = $flight.find('.js-upgrade-cugo-container.cpm-offer');\n\n    var isGlobalCugo = $cugoMainTooltips.find('.cugo-icon-image').is(':visible');\n    var isGlobalCpm = $cugoMainTooltips.find('.cpm-offer-class-logo').is(':visible');\n\n    if ($flight.hasClass(_CNAME_ + '-offers-redesigned')) return;\n\n    $flight.addClass(_CNAME_ + '-offers-redesigned');\n    addFlightHeader($flight, isGlobalCugo, isGlobalCpm, isGlobalMilesUpgrade);\n\n    if (isGlobalCugo) {\n      $cugoMainTooltips.each(rearrangeTooltips);\n    }\n\n    if (isGlobalCpm) {\n      $cpmMainTooltips.each(rearrangeTooltips);\n    }\n\n    if (isGlobalMilesUpgrade) {\n      $globalMilesTooltips.each(rearrangeTooltips);\n    }\n  }\n\n  function addFlightHeader($flight, isGlobalCugo, isGlobalCpm, isGlobalMilesUpgrade) {\n    if ((!isGlobalCugo && !isGlobalCpm) && isGlobalMilesUpgrade) {\n      $flight.prepend('<div class=\"lc-em278-miles-header\">Miles upgrade offers available</div>');\n\n      return;\n    }\n\n    if (isGlobalCugo) {\n      if (isGlobalMilesUpgrade) {\n        $flight.prepend('<div class=\"lc-em278-cugo-header lc-em278-cugo-header--with-miles-upgrade\">Offers exist on some fares</div>');\n      } else {\n        $flight.prepend('<div class=\"lc-em278-cugo-header\">Promo</div>');\n      }\n    }\n\n    if (isGlobalCpm) {\n      if (isGlobalMilesUpgrade) {\n        $flight.prepend('<div class=\"lc-em278-cpm-header lc-em278-cpm-header--with-miles-upgrade\">Skywards offers available</div>');\n      } else {\n        $flight.prepend('<div class=\"lc-em278-cpm-header\">Cash+Miles</div>');\n      }\n    }\n  }\n\n  function rearrangeTooltips() {\n    var $tooltip = $(this);\n    var $classOption = $tooltip.parent();\n    var $priceDetail = $classOption.find('.ts-fbr-option__price-detail');\n    var className = $classOption.data('class').toLowerCase();\n    var expandableTableId = $classOption.data('target');\n    var $expandableTable = $('.ts-fbr-flight-list-row__transition[data-target=\"' + expandableTableId + '\"]');\n    var $selectBtns = $expandableTable.find('.ts-fbr-brand-table__select-btn');\n    var isMilesUpgrade = $classOption.find('.miles-upgrade-icon').length && $classOption.find('.miles-upgrade-icon').is(':visible');\n\n    if (isMilesUpgrade) {\n      $classOption.addClass('lc-em278-with-miles-upgrade');\n    }\n\n    $priceDetail.after($tooltip);\n\n    waitFor(function() {\n      return $expandableTable.find('.js-promo-miles').length;\n    }, 3000)\n      .then(function() {\n        showOffersPill($tooltip, $expandableTable, className);\n      })\n      .catch(function() {\n        showOffersPill($tooltip, $expandableTable, className);\n      });\n\n    $selectBtns.each(addOfferFooter);\n  }\n\n  function addOfferFooter() {\n    var $selectBtn = $(this);\n    var selectBtnId = $selectBtn.data('target');\n    var $selectBtnHeader = $selectBtn.parents('.ts-fbr-flight-list-row__transition').find('.ts-fbr-brand-table__col-header[data-index=\"' + selectBtnId + '\"]');\n    var isCugo = $selectBtnHeader.find('#ts-tooltip-cugo').length;\n    var isCpm = $selectBtnHeader.find('#ts-tooltip-cpm-offer').length;\n    var cpmOfferData = $selectBtnHeader.find('#ts-tooltip-cpm-offer').text().match(/\\d+((.|,)\\d+)?/g, '');\n    var currency = $('.ts-fbr-brand-table__price-currency').eq(0).text().trim();\n\n    if ($selectBtn.next().hasClass('lc-em278-offfer-footer')) return;\n\n    if (isCugo) {\n      $selectBtn.after('<div class=\"lc-em278-offfer-footer lc-em278-cugo-footer-container\"><span class=\"lc-em278-cugo-footer\">Promo code applied</span></div>');\n      $selectBtn.addClass(_CNAME_  + 'has-offer-footer');\n    }\n\n    if (isCpm) {\n      $selectBtn.after('<div class=\"lc-em278-offfer-footer lc-em278-cpm-footer-container\"><span class=\"lc-em278-cpm-footer\">' + currency  + ' ' + cpmOfferData[1]  + ' per ' + cpmOfferData[0] + ' miles</span></div>');\n      $selectBtn.addClass(_CNAME_  + 'has-offer-footer');\n    }\n  }\n\n  function getOffersNumber($expandableTable) {\n    var cugoOffers = $expandableTable.find('#ts-tooltip-cugo').length ? 1 : 0;\n    var cpmOffers = $expandableTable.find('.ts-fbr-tooltip-container.cpm-offer').length ? 1 : 0;\n    var specialOffers = $expandableTable.find('.js-promo-miles > a > strong').filter(function() {\n      return $(this).text().toLowerCase().indexOf('special offer') !== -1;\n    }).length ? 1 : 0;\n\n    return {\n      totalNumber: cugoOffers + cpmOffers + specialOffers,\n      specialOffersNumber: specialOffers\n    };\n  }\n\n  function showOffersPill($tooltip, $expandableTable, className) {\n    var numberOfOffers = getOffersNumber($expandableTable);\n    var $classOption = $tooltip.parent();\n    var $isMilesUpgrade = $classOption.hasClass('lc-em278-with-miles-upgrade');\n    var $isCpmTooltip = $tooltip.hasClass('cpm-offer');\n\n    if ($tooltip.hasClass('lc-em278-main-tooltip')) return;\n\n    if (!numberOfOffers.totalNumber && $isMilesUpgrade) {\n      $tooltip\n        .addClass('lc-em278-miles-upg-only lc-em278-main-tooltip--' + className)\n        .find('a')\n        .html('Miles Upgrade');\n      return;\n    }\n\n    if (!numberOfOffers.totalNumber) {\n      // eslint-disable-next-line consistent-return\n      $tooltip.parent().addClass('lc-em278-no-offers');\n      $tooltip.css('display', 'none');\n      return;\n    }\n\n    if (numberOfOffers.specialOffersNumber) {\n      $tooltip.find('.ts-tooltip .miles-upg-tooltip-info').after(\n        '<div class=\"lc-em278-tooltip-miles-container\">' +\n        '<div class=\"lc-em278-tooltip-miles-header lc-em278-miles-header\">Miles upgrade offers available</div>' +\n        '<div class=\"lc-em278-tooltip-miles-text\">Reduced Miles upgrade offers exist on some fare brands.</div>' +\n        '</div>'\n      );\n    }\n\n    $tooltip\n      .addClass('lc-em278-main-tooltip lc-em278-main-tooltip--' + className)\n      .find('a')\n      // eslint-disable-next-line no-nested-ternary\n      .html($isMilesUpgrade ? 'Offers (' + numberOfOffers.totalNumber  + ')' : ($isCpmTooltip ? 'Cash + Miles' : 'Promo'));\n  }\n\n\n  // // ................................................................... original content - em263 ...................................................................\n\n  function renderElements() {\n    renderClassTabs('outbound');\n    renderClassTabs('inbound');\n    renderFlightSelectionElements();\n  }\n\n  function filterSearchResults() {\n    hideNotMatchingFlights('outbound', selectedClasses.outbound);\n    hideNotMatchingFlights('inbound', selectedClasses.inbound);\n  }\n\n  function addListeners() {\n    addListenersToClassTabs();\n    addListenerToOpenSelectionElements();\n    addListenersToSelectFareElements();\n    addListenersToShowMoreElements();\n    addListenersToSortByElements();\n    addListenerToFilterElement();\n  }\n\n  function renderClassTabs(direction) {\n    var flightListElement = document.querySelector('.' + direction + '-list .ts-fbr-flight-list__body');\n    var flightClasses = getFlightClasses(direction);\n    var html = [\n      '<div class=\"' + _CNAME_ + '-flight-classes-tabs\">',\n      '<p>Show prices for:</p>',\n      flightClasses.map(function(el) {\n        return '<div class=\"' + _CNAME_ + '-class-tab ' + (selectedClasses[direction] === el.toLowerCase() ? (_CNAME_ + '-class-tab-selected') : '') + '\" data-' + _CNAME_ + '-class=\"' + el.toLowerCase() + '\"' + '\" data-' + _CNAME_ + '-direction=\"' + direction + '\">' + el + '</div>';\n      }).join(''),\n      '</div>',\n    ].join('');\n\n    if (flightListElement) {\n      flightListElement.insertAdjacentHTML('beforebegin', html);\n      flightListElement.dataset.lcEm278Classes = flightClasses.length;\n    }\n  }\n\n  function getFlightClasses(direction) {\n    var flightClasses = [].slice.call(document.querySelectorAll('.' + direction + '-list strong.ts-fbr-option__class')).map(function(el) {\n      return el.innerText.trim();\n    });\n    var uniqueFlightClasses = flightClasses.filter(function(el, index) {\n      return flightClasses.indexOf(el) === index;\n    });\n\n    return uniqueFlightClasses;\n  }\n\n  function hideNotMatchingFlights(direction, selectedClass) {\n    var allFlightTileElements = [].slice.call(document.querySelectorAll('.' + direction + '-list div.ts-fbr-option'));\n    var notMatchingFlightTileElements = allFlightTileElements.filter(function(el) {\n      return el.querySelector('strong.ts-fbr-option__class') && el.querySelector('strong.ts-fbr-option__class').textContent.toLowerCase().indexOf(selectedClass) === -1;\n    });\n    var flightRowElements = [].slice.call(document.querySelectorAll('.' + direction + '-list div.ts-fbr-flight-list-row'));\n    var capitalizedSelectedClass = selectedClass.charAt(0).toUpperCase() + selectedClass.slice(1);\n    var openFlightRowLinkElement = document.querySelector('.' + direction + '-list div.lc-em278-options-open div[data-class=\"' + capitalizedSelectedClass + '\"]:not(.ts-fbr-option--opened) a');\n\n    allFlightTileElements.forEach(function(el) {\n      toggleClass(el, 'remove', '-overlay-hidden');\n    });\n\n    notMatchingFlightTileElements.forEach(function(el) {\n      toggleClass(el, 'add', '-overlay-hidden');\n    });\n\n    flightRowElements.forEach(function(el) {\n      el.dataset.lcEm278ActiveTab = selectedClass;\n    });\n\n    openFlightRowLinkElement && openFlightRowLinkElement.click();\n  }\n\n  function highlightSelectedTab(node) {\n    var allTabElements = [].slice.call(document.querySelectorAll('.' + _CNAME_ + '-class-tab[data-' + _CNAME_ + '-direction=\"' + node.dataset.lcEm278Direction + '\"]'));\n\n    allTabElements.forEach(function(el) {\n      toggleClass(el, 'remove', '-class-tab-selected');\n    });\n    toggleClass(node, 'add', '-class-tab-selected');\n  }\n\n  function addListenersToClassTabs() {\n    var classTabElements = [].slice.call(document.querySelectorAll('.' + _CNAME_ + '-class-tab'));\n\n    classTabElements.forEach(function(el) {\n      el.addEventListener('click', classTabsHandler(el));\n    });\n  }\n\n  function classTabsHandler(el) {\n    return function() {\n      var clickedClass = el.dataset.lcEm278Class;\n      var direction = el.dataset.lcEm278Direction;\n      var currentlySelectedClass = selectedClasses[direction];\n\n      if (clickedClass !== currentlySelectedClass) {\n        hideNotMatchingFlights(direction, clickedClass);\n        highlightSelectedTab(el);\n        selectedClasses[direction] = clickedClass;\n      }\n    };\n  }\n\n  function renderFlightSelectionElements() {\n    var flightRowElements = [].slice.call(document.querySelectorAll('div:not(.' + _CNAME_ + '-redesigned).ts-fbr-flight-list-row'));\n    var selectedFlightElement = [\n      '<div class=\"' + _CNAME_ + '-selected-flight\"></div>',\n      '<div class=\"' + _CNAME_ + '-selected-mark\">',\n      '<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"40\" height=\"40\" viewBox=\"0 0 40 40\"><g fill=\"none\" fill-rule=\"evenodd\"><path fill=\"#15700E\" d=\"M0 0h37c1.657 0 3 1.343 3 3v37L0 0z\" transform=\"translate(-1212 -1288) translate(149 754) translate(0 190) translate(3 344) translate(1060)\"></path><path fill=\"#FFF\" d=\"M32 9.626L30.429 8 26.033 12.822 23.646 10.475 22 12.101 24.387 14.374 26.033 16 27.605 14.374z\" transform=\"translate(-1212 -1288) translate(149 754) translate(0 190) translate(3 344) translate(1060)\"></path></g></svg>',\n      '</div>'\n    ].join('');\n\n    flightRowElements.forEach(function(el) {\n      var flightTileEleme