\n        teens,\n        cabinClassCode,\n      } = searchData;\n      let { returnDate } = searchData;\n      const EKHCabinClassMapping = {\n        Y: 1,\n        J: 8,\n        F: 4,\n      };\n      let cabinClass = cabinClassCode;\n      if (cabinClass && cabinClass in EKHCabinClassMapping) {\n        cabinClass = EKHCabinClassMapping[cabinClass];\n      } else {\n        cabinClass = 1;\n      }\n      if (!returnDate) {\n        returnDate = new Date(\n          new Date().setDate(new Date(departureDate).getDate() + 7)\n        );\n        returnDate =\n          returnDate.getFullYear() +\n          "-" +\n          (returnDate.getMonth() + 1) +\n          "-" +\n          returnDate.getDate();\n      }\n      let promises = [\n        fetch(\n          `${HOST}/service/content/api/v4.0/resources/eol${publication}/Experiences/Emirates-Holidays/Bundled-Offers`\n        )\n          .then((response) => {\n            return response.json();\n          })\n          .then((results) => {\n            let resources = null;\n            if (results) {\n              resources = results.results;\n            }\n            return resources;\n          }),\n        fetch(\n          `${HOST}/service/holidays/destinations?countryCode=gb&cultureCode=en-gb&term=${destinationCode.toLowerCase()}`\n        ).then((response) => {\n          return response.json();\n        }),\n      ];\n\n      Promise.all(promises)\n        .then(([resources, holidayDestination]) => {\n          if (\n            resources &&\n            Object.keys(resources).length &&\n            holidayDestination &&\n            holidayDestination.results.length &&\n            holidayDestination.results[0].iataCode\n          ) {\n            const children = [];\n            if (teens > 0) {\n              for (var i = 0; i < teens; i++) {\n                children.push(13);\n              }\n            }\n            if (childs > 0) {\n              for (var i = 0; i < childs; i++) {\n                children.push(11);\n              }\n            }\n            if (infants > 0) {\n              for (var i = 0; i < infants; i++) {\n                children.push(1);\n              }\n            }\n            const destinationInfo =\n              holidayDestination.results[0].iataCode.split("-");\n            fetch(\n              `https://www.emiratesholidays.com/api/search/ext/getdeeplink`,\n              {\n                method: "POST",\n                headers: {\n                  Accept: "application/json",\n                  "Content-Type": "application/json",\n                },\n                body: JSON.stringify({\n                  TenantId: 3,\n                  CountryCode: originCountry,\n                  CultureCode: cultureCode,\n                  StartDate: departureDate,\n                  EndDate: returnDate,\n                  Rooms: [\n                    {\n                      Adults: adults,\n                      childAges: children,\n                    },\n                  ],\n                  Destination: {\n                    type: destinationInfo[0],\n                    id: destinationInfo[1],\n                  },\n                  cabin: cabinClass,\n                  departureAirports: [originCode],\n                  IncludePrices: true,\n                }),\n              }\n            )\n              .then((response) => {\n                return response.json();\n              })\n              .then((holidays) => {\n                if (holidays && holidays.Estabs && holidays.Estabs.length) {\n                  const selectedHolidays = holidays.Estabs.slice(0, 3);\n                  fetch(\n                    "https://www.travelrepublic.co.uk/api/hotels/static/gethotelsbyid",\n                    {\n                      method: "POST",\n                      headers: {\n                        Accept: "application/json",\n                        "Content-Type": "application/json",\n                   